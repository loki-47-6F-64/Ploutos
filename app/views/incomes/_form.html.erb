<%= form_for @income, url: income_path(@income) do |f| %>
 <p>
    <%= f.label "Income per month." %>
    <%= f.text_field :amountRound %>,
      <%= f.text_field :amountDecimal %>
  </p>
  <p>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
